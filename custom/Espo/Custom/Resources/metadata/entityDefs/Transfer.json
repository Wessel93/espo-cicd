{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "transferValue": {
            "type": "currency",
            "isCustom": true,
            "audited": true
        },
        "status": {
            "type": "enum",
            "options": [
                "planned",
                "approved",
                "pending",
                "successful",
                "failed"
            ],
            "style": {
                "planned": "primary",
                "approved": "info",
                "pending": "warning",
                "successful": "success",
                "failed": "danger"
            },
            "default": "planned",
            "isCustom": true,
            "audited": true
        },
        "combined": {
            "type": "link"
        },
        "deliveryMechanism": {
            "type": "enum",
            "options": [
                "IBAN",
                "cashPickUp",
                "digitalPickUp",
                ""
            ],
            "style": {
                "IBAN": null,
                "cashPickUp": null,
                "digitalPickUp": null,
                "": null
            },
            "audited": true,
            "isCustom": true
        },
        "transferType": {
            "type": "enum",
            "options": [
                "deposit",
                "rent",
                "disabilityAllowance",
                "utilityAllowance",
                ""
            ],
            "style": {
                "deposit": null,
                "rent": null,
                "disabilityAllowance": null,
                "utilityAllowance": null,
                "": null
            },
            "isCustom": true
        },
        "transferDate": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "cashDistribution": {
            "type": "link"
        },
        "transferTimeActual": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 30,
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "combined": {
            "type": "belongsTo",
            "foreign": "transfers",
            "entity": "Combined",
            "audited": true,
            "isCustom": true
        },
        "cashDistribution": {
            "type": "belongsTo",
            "foreign": "transfers",
            "entity": "CashDistribution",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}