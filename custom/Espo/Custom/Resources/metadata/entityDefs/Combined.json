{
    "fields": {
        "name": {
            "type": "varchar",
            "required": false,
            "pattern": "$noBadCharacters",
            "options": []
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "affectedHousehold": {
            "type": "link"
        },
        "accommodation": {
            "type": "link"
        },
        "contractStart": {
            "notNull": false,
            "type": "date",
            "default": "javascript: return this.dateTime.getToday();",
            "isCustom": true
        },
        "contractEnd": {
            "notNull": false,
            "type": "date",
            "default": "javascript: return this.dateTime.getToday();",
            "isCustom": true
        },
        "selectedAcc": {
            "type": "enum",
            "options": [
                "hostFam",
                "rental",
                ""
            ],
            "style": {
                "hostFam": null,
                "rental": null,
                "": null
            },
            "isCustom": true
        },
        "statusMatch": {
            "type": "enum",
            "options": [
                "new",
                "pending",
                "confirmed",
                "completed",
                ""
            ],
            "style": {
                "new": "primary",
                "pending": "warning",
                "confirmed": "success",
                "completed": "info",
                "": null
            },
            "isCustom": true,
            "displayAsLabel": true
        },
        "paymentsQuestion": {
            "notNull": true,
            "type": "bool",
            "isCustom": true
        },
        "personPay": {
            "type": "enum",
            "options": [
                "company",
                "propertyOwner",
                "hostfam",
                "affectedHH",
                ""
            ],
            "style": {
                "company": null,
                "propertyOwner": null,
                "hostfam": null,
                "affectedHH": null,
                "": null
            },
            "isCustom": true
        },
        "contractRental": {
            "type": "file",
            "sourceList": [],
            "accept": [],
            "isCustom": true
        },
        "contractHostGuest": {
            "type": "file",
            "sourceList": [],
            "accept": [],
            "isCustom": true
        },
        "bank": {
            "type": "link"
        },
        "swiftCode": {
            "readOnly": true,
            "type": "foreign",
            "link": "bank",
            "field": "bankSwiftCode",
            "view": "views/fields/foreign-varchar",
            "isCustom": true
        },
        "iban": {
            "type": "varchar",
            "maxLength": 150,
            "options": [],
            "isCustom": true,
            "audited": true,
            "pattern": "^[A-Z0-9]+$"
        },
        "ibanVerify": {
            "type": "wysiwyg",
            "audited": true,
            "readOnly": true,
            "isCustom": true
        },
        "validIBAN": {
            "notNull": true,
            "type": "bool",
            "readOnly": true,
            "isCustom": true
        },
        "nrPayments": {
            "type": "int",
            "isCustom": true
        },
        "transfers": {
            "type": "linkMultiple",
            "layoutDetailDisabled": false,
            "layoutMassUpdateDisabled": false,
            "layoutListDisabled": false,
            "noLoad": false,
            "importDisabled": false,
            "exportDisabled": false,
            "customizationDisabled": true,
            "isCustom": true
        },
        "cashDistribution": {
            "type": "link",
            "audited": true
        },
        "matchApproved": {
            "notNull": true,
            "type": "bool",
            "isCustom": true
        },
        "transferApproved": {
            "notNull": true,
            "type": "bool",
            "isCustom": true
        },
        "monitoring": {
            "type": "linkMultiple",
            "layoutDetailDisabled": false,
            "layoutMassUpdateDisabled": false,
            "layoutListDisabled": false,
            "noLoad": false,
            "importDisabled": false,
            "exportDisabled": false,
            "customizationDisabled": false,
            "isCustom": true
        },
        "addressMatch": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "audited": true,
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "affectedHousehold": {
            "type": "belongsTo",
            "foreign": "combineds",
            "entity": "AffectedHousehold",
            "audited": true,
            "isCustom": true
        },
        "accommodation": {
            "type": "belongsTo",
            "foreign": "combineds",
            "entity": "Accommodation",
            "audited": true,
            "isCustom": true
        },
        "contractRental": {
            "type": "belongsTo",
            "entity": "Attachment",
            "skipOrmDefs": true,
            "utility": true
        },
        "contractHostGuest": {
            "type": "belongsTo",
            "entity": "Attachment",
            "skipOrmDefs": true,
            "utility": true
        },
        "bank": {
            "type": "belongsTo",
            "foreign": "combineds",
            "entity": "Bank",
            "audited": false,
            "isCustom": true
        },
        "transfers": {
            "type": "hasMany",
            "foreign": "combined",
            "entity": "Transfer",
            "audited": false,
            "isCustom": true
        },
        "cashDistribution": {
            "type": "belongsTo",
            "foreign": "combineds",
            "entity": "CashDistribution",
            "audited": false,
            "isCustom": true
        },
        "monitoring": {
            "type": "hasMany",
            "foreign": "combined",
            "entity": "Monitoring",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}